  <!-- 安全是否启用安全的url
  何为安全的url：如果页面有参数，则防止参数篡改！
  -->
  <add key="Security.IsEnableSafeUrl" value="true"/>
  <!--分页默认每页显示的纪录数量-->
  <add key="PageSize" value="10"/>
  <!-- 静态资源根目录 -->
  <add key="StaticResourceRoot" value="~/App_Themes/ThemeAdmin"/>


